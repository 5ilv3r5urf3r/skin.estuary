<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">50</defaultcontrol>
	<backgroundcolor>0</backgroundcolor>
	<views>50</views>
	<controls>
		<include>DefaultBackground</include>
		<include>PlaylistWindowButtons</include>
		<include>PlaylistViewtype</include>
				<control type="image">
			<include>OpenClose_Right</include>
			<include>Visible_Right</include>
			<posy>163</posy>
			<left>1324</left>
					<width>506</width>
					<height>742</height>
					<fadetime>200</fadetime>
					<aspectratio aligny="bottom">keep</aspectratio>
					<texture fallback="DefaultVideo.png" background="true">$VAR[PosterVar]</texture>
					<bordertexture border="20">overlays/shadow.png</bordertexture>
					<bordersize>20</bordersize>
				</control>
		<include name="TopBar">
			<param name="breadcrumbs_label" value="Video Playlist" />
			<param name="breadcrumbs_icon" value="icons/settings/video.png" />
		</include>
		<include>BottomBar</include>
	</controls>
</window>
