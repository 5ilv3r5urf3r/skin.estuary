<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol>3</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <controls>
        <include>DefaultBackground</include>
        <control type="group">
            <left>110</left>
            <top>0</top>
            <control type="group">
                <include>OpenClose_FadeSlide_Left</include>
                <control type="image">
                    <description>Menu Panel</description>
                    <left>0</left>
                    <posy>158</posy>
                    <width>400</width>
                    <height>752</height>
                    <texture border="2">dialogs/dialog-bg.png</texture>
                </control>
                <control type="grouplist" id="3">
                    <description>button area</description>
                    <left>0</left>
                    <top>160</top>
                    <width>400</width>
                    <height>700</height>
                    <itemgap>0</itemgap>
                    <usecontrolcoords>true</usecontrolcoords>
                    <onleft>5</onleft>
                    <onright>5</onright>
                    <onup>3</onup>
                    <ondown>20</ondown>
                </control>
                <control type="button" id="10">
                    <description>Default Category Button</description>
                    <left>2</left>
                    <height>75</height>
                    <width>396</width>
                    <textoffsetx>20</textoffsetx>
                    <align>left</align>
                    <aligny>center</aligny>
                    <font>font13_title</font>
                    <textcolor>white</textcolor>
                    <focusedcolor>white</focusedcolor>
                    <texturefocus border="150,1,150,1">lists/focus.png</texturefocus>
                    <texturenofocus border="40">lists/separator.png</texturenofocus>
                    <pulseonselect>false</pulseonselect>
                </control>
                <control type="radiobutton" id="20">
                    <width>400</width>
                    <left>0</left>
                    <top>792</top>
                    <height>120</height>
                    <align>left</align>
                    <aligny>center</aligny>
                    <onclick>SettingsLevelChange</onclick>
                    <font>font13</font>
                    <textoffsetx>100</textoffsetx>
                    <textoffsety>0</textoffsety>
                    <texturefocus border="40">buttons/button-theme-fo.png</texturefocus>
                    <texturenofocus border="40">buttons/button-nofo.png</texturenofocus>
                    <radioposx>40</radioposx>
                    <radioposy>0</radioposy>
                    <radiowidth>40</radiowidth>
                    <radioheight>40</radioheight>
                    <onleft>5</onleft>
                    <onright>5</onright>
                    <onup>3</onup>
                    <ondown>3</ondown>
                    <textureradioonfocus>icons/settings.png</textureradioonfocus>
                    <textureradioonnofocus>icons/settings.png</textureradioonnofocus>
                    <textureradioofffocus>icons/settings.png</textureradioofffocus>
                    <textureradiooffnofocus>icons/settings.png</textureradiooffnofocus>
                </control>
            </control>
            <control type="group">
                <include>OpenClose_FadeSlide_Right</include>
                <control type="image">
                    <left>440</left>
                    <top>158</top>
                    <width>1260</width>
                    <height>752</height>
                    <texture border="2">dialogs/dialog-bg.png</texture>
                </control>
                <control type="image">
                    <description>Dialog Header image</description>
                    <left>442</left>
                    <top>802</top>
                    <width>1256</width>
                    <height>3</height>
                    <texture colordiffuse="button_focus" border="2">dialogs/separator.png</texture>
                </control>
                <!-- <control type="group">
                     <left>500</left>
                     <top>100</top>
                     <include name="DialogBackgroundCommons">
                     <param name="DialogBackgroundWidth" value="1300" />
                     <param name="DialogBackgroundHeight" value="868" />
                     <param name="DialogHeaderLabel" value="$LOCALIZE[24003]" />
                     <param name="DialogHeaderId" value="2" />
                     <param name="darken_bg" value="false" />
                     </include>
                     </control> -->
                <control type="image">
                    <posx>1045</posx>
                    <posy>910</posy>
                    <width>28</width>
                    <height>28</height>
                    <texture>overlays/arrowdown.png</texture>
                    <animation effect="zoom" start="100,0" end="100,100" center="0,910" time="200">VisibleChange</animation>
                    <animation effect="zoom" start="100,0" end="100,100" delay="300" center="0,910" time="200">WindowOpen</animation>
                    <visible>Container(5).HasNext</visible>
                </control>
                <control type="image">
                    <posx>1045</posx>
                    <posy>130</posy>
                    <width>28</width>
                    <height>28</height>
                    <texture flipy="true">overlays/arrowdown.png</texture>
                    <animation effect="zoom" start="100,0" end="100,100" center="0,158" time="200">VisibleChange</animation>
                    <animation effect="zoom" start="100,0" end="100,100" delay="300" center="0,158" time="200">WindowOpen</animation>
                    <visible>Container(5).HasPrevious</visible>
                </control>
                <control type="grouplist" id="5">
                    <description>control area</description>
                    <left>442</left>
                    <top>160</top>
                    <width>1256</width>
                    <height>610</height>
                    <itemgap>0</itemgap>
                    <pagecontrol>60</pagecontrol>
                    <onleft>3</onleft>
                    <onright>60</onright>
                    <onup>5</onup>
                    <ondown>5</ondown>
                </control>
                <control type="scrollbar" id="60">
                    <left>1060</left>
                    <top>60</top>
                    <width>25</width>
                    <height>435</height>
                    <texturesliderbackground border="0,14,0,14">ScrollBarV.png</texturesliderbackground>
                    <texturesliderbar border="0,14,0,14">ScrollBarV_bar.png</texturesliderbar>
                    <texturesliderbarfocus border="0,14,0,14">ScrollBarV_bar_focus.png</texturesliderbarfocus>
                    <textureslidernib>ScrollBarNib.png</textureslidernib>
                    <textureslidernibfocus>ScrollBarNib.png</textureslidernibfocus>
                    <onleft>5</onleft>
                    <onright>3</onright>
                    <showonepage>false</showonepage>
                    <orientation>vertical</orientation>
                </control>
                <control type="image">
                    <description>separator image</description>
                    <left>290</left>
                    <top>525</top>
                    <width>750</width>
                    <height>30</height>
                    <texture>separator2.png</texture>
                    <colordiffuse>40FFFFFF</colordiffuse>
                </control>
                <control type="textbox" id="6">
                    <description>description area</description>
                    <left>480</left>
                    <top>810</top>
                    <width>1150</width>
                    <height>83</height>
                    <font>font12</font>
                    <align>justify</align>
                    <textcolor>button_focus</textcolor>
                    <autoscroll time="3000" delay="4000" repeat="5000">true</autoscroll>
                </control>
            </control>
        </control>
        <control type="button" id="7">
            <description>Default Button</description>
            <left>2</left>
            <width>1256</width>
            <include>DefaultSettingButton</include>
            <texturefocus border="150,1,150,1">lists/focus.png</texturefocus>
            <texturenofocus border="4">lists/separator.png</texturenofocus>
        </control>
        <control type="radiobutton" id="8">
            <description>Default RadioButton</description>
            <left>2</left>
            <width>1256</width>
            <include>DefaultSettingButton</include>
            <texturefocus border="150,1,150,1">lists/focus.png</texturefocus>
            <texturenofocus border="40">lists/separator.png</texturenofocus>
        </control>
        <control type="spincontrolex" id="9">
            <description>Default SpinControlex</description>
            <left>2</left>
            <width>1256</width>
            <include>DefaultSettingButton</include>
            <texturefocus border="150,1,150,1">lists/focus.png</texturefocus>
            <texturenofocus border="40">lists/separator.png</texturenofocus>
            <aligny>center</aligny>
            <reverse>yes</reverse>
        </control>
        <control type="sliderex" id="13">
            <description>Default Slider</description>
            <left>2</left>
            <width>1256</width>
            <include>DefaultSettingButton</include>
            <texturefocus border="150,1,150,1">lists/focus.png</texturefocus>
            <texturenofocus border="40">lists/separator.png</texturenofocus>
            <aligny>center</aligny>
        </control>
        <control type="image" id="11">
            <description>separator image</description>
            <height>3</height>
            <texture colordiffuse="grey" border="2">dialogs/separator.png</texture>
        </control>
        <control type="label" id="14">
            <description>Default Label</description>
            <left>2</left>
            <width>1256</width>
            <textoffsetx>45</textoffsetx>
            <top>0</top>
            <height>30</height>
            <label>-</label>
            <align>center</align>
            <aligny>bottom</aligny>
            <font>font13_title</font>
            <textcolor>grey</textcolor>
            <shadowcolor>black</shadowcolor>
        </control>
        <control type="image">
            <description>Section header image</description>
            <left>20</left>
            <top>3</top>
            <width>35</width>
            <height>35</height>
            <aspectratio>keep</aspectratio>
            <texture>icon_system.png</texture>
        </control>
        <include>TopBar</include>
        <include>BottomBar</include>
        <control type="group">
            <include>OpenClose_FadeSlide_Down</include>
            <control type="image">
                <description>breadcrumbs image</description>
                <left>0</left>
                <top>10</top>
                <width>80</width>
                <height>80</height>
                <texture colordiffuse="button_focus">$VAR[SettingsSectionIcon]</texture>
            </control>
            <control type="label" id="2">
                <description>breadcrumbs label</description>
                <left>80</left>
                <top>20</top>
                <width>1300</width>
                <height>30</height>
                <font>font16</font>
                <align>left</align>
                <aligny>center</aligny>
                <textcolor>white</textcolor>
                <shadowcolor>black</shadowcolor>
            </control>
        </control>
    </controls>
</window>
