<?xml version="1.0" encoding="UTF-8"?>
<window>
	<animation effect="fade" start="0" end="100" time="300">WindowOpen</animation>
	<animation effect="fade" start="100" end="0" time="200">WindowClose</animation>
	<defaultcontrol>noop</defaultcontrol>
	<controls>
		<control type="group" id="1">
			<control type="group" id="1">
				<left>30</left>
				<top>870</top>
				<visible>!VideoPlayer.Content(LiveTV) | [VideoPlayer.Content(LiveTV) + VideoPlayer.HasEpg]</visible>
				<animation effect="fade" start="0" end="100" time="400">VisibleChange</animation>
				<control type="progress" id="1">
					<description>ProgressbarCache</description>
					<left>0</left>
					<top>0</top>
					<width>1860</width>
					<height>30</height>
					<info>Player.ProgressCache</info>
					<reveal>true</reveal>
					<texturebg border="3" colordiffuse="00FFFFFF">colors/white50.png</texturebg>
					<midtexture colordiffuse="button_focus">colors/white50.png</midtexture>
					<animation effect="fade" start="100" end="80" time="0" condition="true">Conditional</animation>
					<visible>!Window.IsVisible(videoosd)</visible>
				</control>
				<control type="progress" id="1">
					<description>Progressbar</description>
					<left>0</left>
					<top>0</top>
					<width>1860</width>
					<height>30</height>
					<info>Player.Progress</info>
					<reveal>true</reveal>
					<texturebg border="3" colordiffuse="80FFFFFF">colors/white50.png</texturebg>
					<midtexture colordiffuse="button_focus">colors/white50.png</midtexture>
					<visible>!Window.IsVisible(videoosd)</visible>
				</control>
			</control>
			<control type="image">
				<description>Info Panel</description>
				<left>30</left>
				<top>650</top>
				<width>1860</width>
				<height>190</height>
				<texture colordiffuse="B0FFFFFF">colors/black.png</texture>
			</control>
			<control type="image" id="1">
				<description>TV Cover</description>
				<right>30</right>
				<top>540</top>
				<width>200</width>
				<height>300</height>
				<texture fallback="DefaultVideoCover.png">$INFO[Player.Art(tvshow.poster)]</texture>
				<aspectratio aligny="bottom">keep</aspectratio>
				<visible>VideoPlayer.Content(Episodes)</visible>
			</control>
			<control type="image" id="1">
				<description>General Cover</description>
				<right>30</right>
				<top>650</top>
				<width>200</width>
				<height>190</height>
				<texture fallback="DefaultVideoCover.png">$INFO[Player.Art(thumb)]</texture>
				<aspectratio aligny="bottom">scale</aspectratio>
				<visible>![VideoPlayer.Content(Movies) | VideoPlayer.Content(LiveTV) | VideoPlayer.Content(Episodes)]</visible>
			</control>
			<control type="image" id="1">
				<description>Movie Cover</description>
				<right>30</right>
				<top>540</top>
				<width>200</width>
				<height>300</height>
				<texture fallback="DefaultVideoCover.png">$INFO[Player.Art(thumb)]</texture>
				<aspectratio aligny="bottom">keep</aspectratio>
				<visible>VideoPlayer.Content(Movies)</visible>
			</control>
			<control type="image" id="1">
				<description>LiveTV Cover</description>
				<right>30</right>
				<top>650</top>
				<width>200</width>
				<height>190</height>
				<texture fallback="DefaultVideoCover.png">$INFO[Player.Art(thumb)]</texture>
				<aspectratio aligny="bottom">scale</aspectratio>
				<visible>VideoPlayer.Content(LiveTV)</visible>
			</control>
			<control type="label" id="1">
				<description>Heading label</description>
				<left>50</left>
				<top>600</top>
				<width>910</width>
				<height>25</height>
				<aligny>center</aligny>
				<font>font30</font>
				<label>[UPPERCASE]$LOCALIZE[31040][/UPPERCASE]: $INFO[Player.Title]</label>
				<shadowcolor>black</shadowcolor>
				<visible>![VideoPlayer.Content(LiveTV) + Player.Recording]</visible>
				<animation effect="slide" start="0,0" end="0,25" time="0" condition="!VideoPlayer.Content(Movies) + !VideoPlayer.Content(Episodes) + !VideoPlayer.Content(MusicVideos) + !VideoPlayer.Content(LiveTV)">conditional</animation>
			</control>
			<control type="group" id="1">
				<left>50</left>
				<top>600</top>
				<control type="label" id="1">
					<description>Music Info label</description>
					<left>20</left>
					<top>30</top>
					<width>910</width>
					<height>25</height>
					<font>font12</font>
					<label>$INFO[VideoPlayer.Artist] $INFO[VideoPlayer.Album, - ]</label>
					<textcolor>grey2</textcolor>
					<shadowcolor>black</shadowcolor>
					<visible>VideoPlayer.Content(MusicVideos)</visible>
				</control>
				<control type="textbox" id="1">
					<left>0</left>
					<top>73</top>
					<width>1600</width>
					<height>146</height>
					<label>$INFO[VideoPlayer.Plot]</label>
					<align>justify</align>
					<autoscroll delay="5000" repeat="7500" time="5000"></autoscroll>
				</control>
				<control type="label" id="1">
					<right>255</right>
					<top>65</top>
					<width min="10" max="840">auto</width>
					<height>44</height>
					<label>$INFO[VideoPlayer.TVShowTitle]$INFO[VideoPlayer.Season, • Season ,]$INFO[VideoPlayer.Episode, • Episode ,]$INFO[Player.Duration, • ,]</label>
					<align>right</align>
					<scroll>true</scroll>
					<visible>VideoPlayer.Content(Episodes)</visible>
				</control>
				<control type="label" id="1">
					<right>255</right>
					<top>65</top>
					<width min="10" max="840">auto</width>
					<height>44</height>
					<label>$INFO[VideoPlayer.Year]$INFO[VideoPlayer.Genre, • ,]$INFO[VideoPlayer.Studio, • ,]$INFO[VideoPlayer.Rating, • ,]$INFO[Player.Duration(mm:ss), • ,]</label>
					<align>right</align>
					<scroll>true</scroll>
					<visible>VideoPlayer.Content(Movies)</visible>
				</control>
				<control type="label" id="1">
					<right>255</right>
					<top>65</top>
					<width min="10" max="840">auto</width>
					<height>44</height>
					<label>$INFO[VideoPlayer.ChannelNumberLabel,$LOCALIZE[19029]: , ]$INFO[VideoPlayer.ChannelName]$INFO[VideoPlayer.NextTitle, • $LOCALIZE[19031]: ]</label>
					<align>right</align>
					<scroll>true</scroll>
					<visible>VideoPlayer.Content(LiveTV)</visible>
				</control>
			</control>
			<control type="group">
				<visible>!Window.IsVisible(videoosd)</visible>
				<animation effect="fade" start="0" end="100" time="400">VisibleChange</animation>
				<control type="image">
					<description>Menu Panel</description>
					<left>30</left>
					<top>900</top>
					<width>1860</width>
					<height>140</height>
					<texture colordiffuse="B0FFFFFF">colors/black.png</texture>
				</control>
				<control type="label" id="1">
					<description>Rating</description>
					<left>20</left>
					<width>155</width>
					<top>925\</top>
					<align>center</align>
					<font>font30_title</font>
					<shadowcolor>text_shadow</shadowcolor>
					<label>$INFO[VideoPlayer.Rating]</label>
				</control>
				<control type="label" id="1">
					<description>Rating label</description>
					<left>20</left>
					<top>970</top>
					<width>155</width>
					<align>center</align>
					<font>font12</font>
					<shadowcolor>text_shadow</shadowcolor>
					<label>Rating</label>
				</control>
				<control type="label" id="1">
					<description>Channel Group label</description>
					<left>50</left>
					<top>900</top>
					<width>1000</width>
					<height>100</height>
					<font>font16</font>
					<shadowcolor>text_shadow</shadowcolor>
					<label>$INFO[VideoPlayer.ChannelGroup,$LOCALIZE[31509]: ]</label>
					<visible>VideoPlayer.Content(LiveTV)</visible>
				</control>
				<control type="label" id="1">
					<description>Player time</description>
					<left>250</left>
					<top>940</top>
					<width>1920</width>
					<height>100</height>
					<font>font16</font>
					<shadowcolor>text_shadow</shadowcolor>
					<label>$INFO[Player.Time]$INFO[Player.Duration, / , •]  $INFO[Player.Chapter,$LOCALIZE[21396]: ]$INFO[Player.ChapterCount, /] • $INFO[Player.FinishTime, Ends: ,]</label>
					<visible>!Player.Seeking</visible>
				</control>
				<control type="grouplist" id="1">
					<right>50</right>
					<top>940</top>
					<align>right</align>
					<width>785</width>
					<height>56</height>
					<itemgap>10</itemgap>
					<orientation>horizontal</orientation>
					<control type="image" id="1">
						<description>Video Resolution Image</description>
						<width>150</width>
						<height>56</height>
						<aspectratio align="center">scale</aspectratio>
						<texture>$INFO[VideoPlayer.VideoResolution,flags/video/,.png]</texture>
					</control>
					<control type="image" id="1">
						<description>ADSP Master Mode Image</description>
						<width>150</width>
						<height>56</height>
						<aspectratio align="right">scale</aspectratio>
						<visible>!IsEmpty(ADSP.MasterOwnIcon)</visible>
						<texture>$INFO[ADSP.MasterOwnIcon]</texture>
					</control>
					<control type="image" id="1">
						<description>Audio Codec Image</description>
						<width>150</width>
						<height>56</height>
						<aspectratio align="center">scale</aspectratio>
						<visible>IsEmpty(ADSP.MasterOverrideIcon)</visible>
						<texture>$INFO[VideoPlayer.AudioCodec,flags/audio/,.png]</texture>
					</control>
					<control type="image" id="1">
						<description>ADSP Audio Codec Override Image</description>
						<width>150</width>
						<height>56</height>
						<aspectratio align="right">scale</aspectratio>
						<visible>!IsEmpty(ADSP.MasterOverrideIcon)</visible>
						<texture>$INFO[ADSP.MasterOverrideIcon]</texture>
					</control>
					<control type="image" id="1">
						<description>Audio Channels Image</description>
						<width>150</width>
						<height>56</height>
						<aspectratio align="center">scale</aspectratio>
						<texture>$INFO[VideoPlayer.AudioChannels,flags/audio/,.png]</texture>
					</control>
				</control>
			</control>
		</control>
	</controls>
</window>
