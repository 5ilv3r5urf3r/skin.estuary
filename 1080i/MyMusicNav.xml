<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol always="true">50</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <backgroundcolor>0</backgroundcolor>
    <views>50,52,500</views>
    <controls>
        <include>DefaultBackground</include>
        <include>View_50_List</include>
        <include name="View_52_IconWall">
            <param name="visible" value="Container.Content() | Container.Content(years) | Container.Content(files) | Container.Content(genres)" />
        </include>
        <include name="View_500_SmallThumb">
            <param name="layouts_include" value="SmallThumbMusicLayouts" />
            <paran name="visible" value="Container.Content(artists) | Container.Content(albums)" />
        </include>
        <include>Media_Menu</include>
        <include name="TopBar">
            <param name="breadcrumbs_label" value="$LOCALIZE[10516]" />
            <param name="breadcrumbs_icon" value="icons/settings/music.png" />
        </include>
        <include>BottomBar</include>
    </controls>
</window>
