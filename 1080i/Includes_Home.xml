    <include name="WidgetListMovies">
        <control type="group">
            <description>Poster Widget including header and arrows</description>
            <height>420</height>
            <posx>0</posx>
            <posy>0</posy>
            <visible>IntegerGreaterThan(Container($PARAM[list_id]).NumItems,0) | Container($PARAM[list_id]).IsUpdating</visible>
            <control type="image">
                <posx>460</posx>
                <posy>200</posy>
                <width>28</width>
                <height>28</height>
                <texture flipx="true">overlays/arrowright.png</texture>
                <animation effect="slide" start="30,0" end="0,0" time="200">VisibleChange</animation>
                <animation effect="slide" start="-150" end="0" delay="640" time="320" tween="cubic">WindowOpen</animation>
                <visible>Container($PARAM[list_id]).HasPrevious + Control.HasFocus($PARAM[list_id])</visible>
            </control>
            <control type="image">
                <posx>1860</posx>
                <posy>200</posy>
                <width>28</width>
                <height>28</height>
                <texture>overlays/arrowright.png</texture>
                <animation effect="slide" start="-30,0" end="0,0" time="200">VisibleChange</animation>
                <animation effect="slide" start="150" end="0" delay="640" time="320" tween="cubic">WindowOpen</animation>
                <visible>Container($PARAM[list_id]).HasNext + Control.HasFocus($PARAM[list_id])</visible>
            </control>
            <control type="label">
                <posx>500</posx>
                <posy>0</posy>
                <width>600</width>
                <height>40</height>
                <label>$PARAM[widget_header]</label>
                <font>font14</font>
                <textcolor>white</textcolor>
                <shadowcolor>22000000</shadowcolor>
                <scroll>false</scroll>
                <align>left</align>
            </control>
            <control type="list" id="$PARAM[list_id]">
                <posx>500</posx>
                <posy>60</posy>
                <width>1400</width>
                <height>400</height>
                <scrolltime tween="cubic" easing="out">500</scrolltime>
                <orientation>horizontal</orientation>
                <onleft>9000</onleft>
                <onright>$PARAM[list_id]</onright>
                <onup>Control.SetFocus($PARAM[onup_id],$INFO[Container($PARAM[list_id]).Position])</onup>
                <onup>$PARAM[onup_id]</onup>
                <ondown>Control.SetFocus($PARAM[ondown_id],$INFO[Container($PARAM[list_id]).Position])</ondown>
                <ondown>$PARAM[ondown_id]</ondown>
                <onback>SetFocus(9000)</onback>
                <preloaditems>1</preloaditems>
                <animation type="WindowOpen">
                    <effect type="fade" start="0" end="100" time="300" tween="sine" delay="300" easing="out" />
                    <effect type="zoom" start="120" end="100" time="400" delay="300" tween="cubic" easing="out" />
                </animation>
                <animation type="WindowClose">
                    <effect type="fade" start="100" end="0" time="300" tween="sine" easing="out" />
                    <effect type="zoom" start="100" end="120" time="300" tween="cubic" easing="out" />
                </animation>
                <itemlayout width="280">
                    <control type="group">
                        <control type="image">
                            <posx>0</posx>
                            <posy>0</posy>
                            <width>220</width>
                            <height>330</height>
                            <aspectratio>stretch</aspectratio>
                            <texture border="2" fallback="DefaultVideo.png">$INFO[ListItem.Art(poster)]</texture>
                            <bordertexture>colors/black.png</bordertexture>
                            <bordersize>4</bordersize>
                        </control>
                    </control>
                </itemlayout>
                <focusedlayout width="280">
                    <control type="group">
                        <animation type="Focus">
                            <effect type="zoom" start="100" end="110" time="200" tween="sine" easing="inout" center="110,150" />
                        </animation>
                        <animation type="Unfocus">
                            <effect type="zoom" start="110" end="100" time="200" tween="sine" easing="inout" center="110,150" />
                        </animation>
                        <control type="image">
                            <posx>0</posx>
                            <posy>0</posy>
                            <width>220</width>
                            <height>330</height>
                            <aspectratio>stretch</aspectratio>
                            <texture border="2" fallback="DefaultVideo.png">$INFO[ListItem.Art(poster)]</texture>
                            <bordertexture>colors/black.png</bordertexture>
                            <bordersize>4</bordersize>
                        </control>
                        <control type="group">
                            <animation type="Focus">
                                <effect type="fade" start="0" end="100" time="200"/>
                            </animation>
                            <animation type="Unfocus">
                                <effect type="fade" start="100" end="0" time="200"/>
                            </animation>
                            <visible>Control.HasFocus($PARAM[list_id])</visible>
                            <control type="image">
                                <posx>0</posx>
                                <posy>270</posy>
                                <width>220</width>
                                <height>60</height>
                                <aspectratio>stretch</aspectratio>
                                <texture>colors/black.png</texture>
                            </control>
                            <control type="label">
                                <posx>110</posx>
                                <posy>270</posy>
                                <width>180</width>
                                <label>$INFO[ListItem.Label]</label>
                                <font>font10</font>
                                <textcolor>white</textcolor>
                                <shadowcolor>22000000</shadowcolor>
                                <scroll>false</scroll>
                                <align>center</align>
                            </control>
                            <control type="label">
                                <posx>110</posx>
                                <posy>294</posy>
                                <width>180</width>
                                <label>$INFO[ListItem.Rating,, [COLOR=44FFFFFF]/[/COLOR] ]$INFO[ListItem.Year]</label>
                                <font>font10</font>
                                <textcolor>white</textcolor>
                                <shadowcolor>22000000</shadowcolor>
                                <scroll>false</scroll>
                                <align>center</align>
                            </control>
                            <control type="image">
                                <posx>0</posx>
                                <posy>0</posy>
                                <width>220</width>
                                <height>330</height>
                                <aspectratio>stretch</aspectratio>
                                <texture background="true" border="8">buttons/thumbnail_focused.png</texture>
                            </control>
                        </control>
                    </control>
                </focusedlayout>
                <content target="$PARAM[widget_target]" limit="10">$PARAM[content_path]</content>
            </control>
        </control>
    </include>
