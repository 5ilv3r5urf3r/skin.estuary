<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="View_50_List">
        <control type="group">
            <description>List Viewtype</description>
            <height>424</height>
            <posx>0</posx>
            <posy>250</posy>
            <!-- <visible>Control.IsVisible(50)</visible>
 -->
            <control type="group">
                <include>OpenClose_FadeSlide_Left</include>
                <posx>50</posx>
                <include name="DialogBackgroundCommons">
                    <param name="DialogBackgroundWidth" value="1000" />
                    <param name="DialogBackgroundHeight" value="700" />
                    <param name="DialogHeaderWidth" value="620" />
                    <param name="DialogHeaderId" value="2" />
                    <param name="darken_bg" value="False" />
                </include>
                <control type="list" id="50">
                    <posx>2</posx>
                    <posy>63</posy>
                    <width>996</width>
                    <height>620</height>
                    <oninfo>$PARAM[list_oninfo]</oninfo>
                    <scrolltime tween="cubic" easing="out">500</scrolltime>
                    <orientation>vertical</orientation>
                    <onleft>50</onleft>
                    <onright>50</onright>
                    <onup>$PARAM[onup_id]</onup>
                    <ondown>$PARAM[ondown_id]</ondown>
                    <onback>SetFocus(50)</onback>
                    <preloaditems>1</preloaditems>
                    <focusedlayout height="70">
                        <control type="image">
                            <posx>0</posx>
                            <posy>0</posy>
                            <width>996</width>
                            <height>71</height>
                            <texture border="90,3,90,3">lists/separator.png</texture>
                        </control>
                        <control type="image">
                            <posx>0</posx>
                            <posy>0</posy>
                            <width>996</width>
                            <height>69</height>
                            <texture border="150,1,150,1">lists/focus.png</texture>
                            <animation effect="fade" start="100" end="0" time="0" condition="!Control.HasFocus(50)">Conditional</animation>
                        </control>
                        <control type="label">
                            <posx>70</posx>
                            <posy>0</posy>
                            <height>70</height>
                            <width>560</width>
                            <align>left</align>
                            <aligny>center</aligny>
                            <font>font14</font>
                            <label>$INFO[ListItem.Label]</label>
                            <selectedcolor>SelectedGreen</selectedcolor>
                            <textcolor>white</textcolor>
                            <shadowcolor>text_shadow</shadowcolor>
                        </control>
                        <control type="image">
                            <posx>0</posx>
                            <posy>-4</posy>
                            <width>70</width>
                            <height>70</height>
                            <texture>lists/arrow-right-nofo.png</texture>
                        </control>
                        <control type="image">
                            <posx>0</posx>
                            <posy>-4</posy>
                            <width>70</width>
                            <height>70</height>
                            <texture>lists/arrow-right-fo.png</texture>
                            <animation effect="fade" start="0" end="100" time="300" delay="0" reversible="false">Focus</animation>
                        </control>
                        <control type="image">
                            <posx>0</posx>
                            <posy>-4</posy>
                            <width>70</width>
                            <height>70</height>
                            <texture>lists/arrow-right-colour-fo.png</texture>
                            <animation effect="fade" start="0" end="100" time="300" delay="0" reversible="false">Focus</animation>
                            <visible>!Control.HasFocus(50)</visible>
                        </control>
                    </focusedlayout>
                    <itemlayout height="70">
                        <control type="image">
                            <posx>0</posx>
                            <posy>0</posy>
                            <width>996</width>
                            <height>71</height>
                            <texture border="90,3,90,3">lists/separator.png</texture>
                        </control>
                        <control type="label">
                            <posx>70</posx>
                            <posy>0</posy>
                            <height>70</height>
                            <width>560</width>
                            <aligny>center</aligny>
                            <font>font14</font>
                            <label>$INFO[ListItem.Label]</label>
                            <selectedcolor>SelectedGreen</selectedcolor>
                            <textcolor>white</textcolor>
                            <shadowcolor>text_shadow</shadowcolor>
                            <scroll>false</scroll>
                        </control>
                        <control type="image">
                            <posx>0</posx>
                            <posy>-4</posy>
                            <width>70</width>
                            <height>70</height>
                            <texture>lists/arrow-right-nofo.png</texture>
                        </control>
                    </itemlayout>
                </control>
            </control>
        </control>
    </include>
</includes>
